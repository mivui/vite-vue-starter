{
  "name": "vite-cli",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "restart": "vite --force",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "eslint": "eslint --fix src/**/*.{ts,tsx,vue}",
    "stylelint": "stylelint src/**/*.{css,less,scss}",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "pinia": "^2.2.0",
    "pinia-plugin-store": "^2.2.6",
    "vue": "^3.4.35",
    "vue-router": "^4.4.2"
  },
  "devDependencies": {
    "@types/node": "^20.14.14",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vitest/coverage-v8": "^2.0.5",
    "@vue/compiler-sfc": "^3.4.35",
    "@vue/test-utils": "^2.4.6",
    "happy-dom": "^14.12.3",
    "sass": "^1.77.8",
    "typescript": "^5.5.4",
    "vite": "^5.3.5",
    "vitest": "^2.0.5",
    "vue-eslint-standard": "^9.0.13",
    "vue-stylelint-standard": "^16.0.6",
    "vue-tsc": "^2.0.29"
  }
}