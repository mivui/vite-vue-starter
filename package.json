{
  "name": "vite-cli",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "restart": "vite --force",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "eslint": "eslint --fix src/**/*.{ts,tsx,vue}",
    "stylelint": "stylelint src/**/*.{css,less,scss}",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "pinia": "^2.3.0",
    "pinia-plugin-store": "^2.3.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vitest/coverage-v8": "^2.1.8",
    "@vue/compiler-sfc": "^3.5.13",
    "@vue/test-utils": "^2.4.6",
    "happy-dom": "^15.11.7",
    "sass": "^1.83.0",
    "typescript": "^5.7.2",
    "vite": "^6.0.5",
    "vitest": "^2.1.8",
    "vue-eslint-standard": "^9.13.0",
    "vue-stylelint-standard": "^16.8.0",
    "vue-tsc": "^2.1.10"
  }
}