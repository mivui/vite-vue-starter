{
  "name": "vite-cli",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "restart": "vite --force",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "eslint": "eslint --fix src/**/*.{ts,tsx,vue}",
    "stylelint": "stylelint src/**/*.{css,less,scss}",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "pinia": "^3.0.2",
    "pinia-plugin-store": "^3.0.2",
    "vue": "^3.5.16",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@types/node": "^22.15.27",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitejs/plugin-vue-jsx": "^4.2.0",
    "@vitest/coverage-v8": "^3.1.4",
    "@vue/compiler-sfc": "^3.5.16",
    "@vue/test-utils": "^2.4.6",
    "happy-dom": "^17.5.6",
    "sass": "^1.89.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.1.4",
    "vue-eslint-standard": "^9.35.0",
    "vue-stylelint-standard": "^16.20.0",
    "vue-tsc": "^2.2.10"
  }
}